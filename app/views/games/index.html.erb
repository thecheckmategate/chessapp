INDEX PAGE 

<% @games.each do |game| %> 

	Name <%= game.name %>
	Created At <%= game.created_at %>
	Description <%= game.description %> 
	White ID <%= game.white_id %>
	Black ID <%= game.black_id %>
	User ID <%= game.user_id %>
	<%= link_to "Join In", join_game_path(game), class: 'btn btn-primary' %>

<% end %> 
