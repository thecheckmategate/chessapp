<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Add a new game</h1>
  <%= simple_form_for @game, url: creator_games_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %> 
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>



